<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuario" table="usuarios">
    <unique-constraints>
      <unique-constraint name="NICKNAME_USER" columns="NICKNAME_USER"/>
      <unique-constraint name="PASSWORD_USER" columns="PASSWORD_USER"/>
    </unique-constraints>
    <id name="nifUser" type="string" column="NIF_USER" length="9">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreUser" type="string" column="NOMBRE_USER" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellido1User" type="string" column="APELLIDO1_USER" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellido2User" type="string" column="APELLIDO2_USER" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fotografiaUser" type="string" column="FOTOGRAFIA_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nicknameUser" type="string" column="NICKNAME_USER" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telfFijoUser" type="decimal" column="TELF_FIJO_USER" precision="9" scale="0" nullable="true"/>
    <field name="telfMovilUser" type="decimal" column="TELF_MOVIL_USER" precision="9" scale="0" nullable="true"/>
    <field name="emailUser" type="string" column="EMAIL_USER" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passwordUser" type="string" column="PASSWORD_USER" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rolUser" type="string" column="ROL_USER" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
