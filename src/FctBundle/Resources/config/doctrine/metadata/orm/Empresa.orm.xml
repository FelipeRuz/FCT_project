<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Empresa" table="empresa">
    <id name="cif" type="string" column="cif" length="9">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomtutor" type="string" column="nomtutor" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="poblacion" type="string" column="poblacion" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cp" type="string" column="cp" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="provincia" type="string" column="provincia" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tlffijo" type="string" column="tlfFijo" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tlfmovil" type="string" column="tlfMovil" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
