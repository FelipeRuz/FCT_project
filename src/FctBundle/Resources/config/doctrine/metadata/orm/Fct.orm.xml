<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Fct" table="fct">
    <indexes>
      <index name="f_keyAlumnos" columns="id_alumno"/>
      <index name="f_keyProf" columns="id_profesor"/>
      <index name="f_keyEmp" columns="id_empresa"/>
    </indexes>
    <id name="codFct" type="integer" column="cod_fct">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fecha" type="date" column="fecha" nullable="false"/>
    <many-to-one field="idAlumno" target-entity="Alumno" fetch="LAZY">
      <join-columns>
        <join-column name="id_alumno" referenced-column-name="id_alumno"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpresa" target-entity="Empresa" fetch="LAZY">
      <join-columns>
        <join-column name="id_empresa" referenced-column-name="id_empresa"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idProfesor" target-entity="Profesor" fetch="LAZY">
      <join-columns>
        <join-column name="id_profesor" referenced-column-name="id_profesor"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
