<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ciclo" table="ciclo">
    <id name="codCiclo" type="integer" column="cod_ciclo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="grado" type="string" column="grado" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="horasfct" type="string" column="HorasFCT" length="4" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
