<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuario" table="usuario">
    <id name="dni" type="string" column="dni" length="9">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipo" type="string" column="tipo" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ape1" type="string" column="ape1" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ape2" type="string" column="ape2" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nick" type="string" column="nick" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tlffijo" type="string" column="tlfFijo" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tlfmovil" type="string" column="tlfMovil" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="foto" type="string" column="foto" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
